{
  "name": "simple-icons-qwik",
  "version": "15.11.2",
  "description": "Simple icon library package for Qwik applications",
  "author": {
    "name": "Luminescent",
    "url": "https://luminescent.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LuminescentDev/simple-icons-qwik.git"
  },
  "license": "LICENSE",
  "keywords": [
    "icon",
    "icons",
    "simple",
    "simple-icons",
    "qwik"
  ],
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=16.8.0 <18.0.0 || >=18.11",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=8.6.12"
  },
  "packageManager": "pnpm@10.15.0",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "pnpm build.icons && qwik build",
    "build.icons": "node --trace-warnings build.ts",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "np",
    "start": "vite --open --mode ssr",
    "sync-version": "node --trace-warnings sync-version.js",
    "qwik": "qwik",
    "test": "echo no tests for now"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.15.0",
    "@eslint/js": "^9.33.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.3.0",
    "eslint": "9.35.0",
    "eslint-plugin-qwik": "^1.15.0",
    "globals": "^16.3.0",
    "np": "^10.2.0",
    "simple-icons": "^15.11.0",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.40.0",
    "undici": "7.14.0",
    "vite": "7.1.3",
    "yargs": "^18.0.0"
  }
}
